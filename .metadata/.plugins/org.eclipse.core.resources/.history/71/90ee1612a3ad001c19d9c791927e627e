package JAVA;

import java.util.Scanner;

public class Main
{
	public static void main(String[] args)
	{
		int a, b;
		Scanner sc = new Scanner(System.in);
		System.out.println("두 정수를 입력하시오 : ");
		a = sc.nextInt();
		b = sc.nextInt();
		
		if (a > b) {
			System.out.println(">");
		}
		else if (a < b) {
			System.out.println("<");
		}
		else 
			System.out.println("==");
		
	}
}
